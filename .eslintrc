{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}